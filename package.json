{
  "name": "jupiter",
  "description": "Vue3向けのコンポーネントライブラリ",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "module": "./dist/jupiter.mjs",
  "main": "./dist/jupiter.umd.js",
  "exports": {
    ".": {
      "import": "./dist/jupiter.mjs",
      "require": "./dist/jupiter.umd.js"
    }
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:unit": "vitest run --environment jsdom --root src/",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.12.4",
    "@mdi/js": "^7.1.96",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "histoire": "^0.12.4",
    "jsdom": "^20.0.3",
    "npm-check-updates": "^16.7.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "9.2.0"
  }
}
